<h2><%= @title %></h2>

<%= f = form_for @changeset, "#",
  id: "url-form",
  phx_target: @myself,
  phx_change: "validate",
  phx_submit: "save" %>

  <%= label f, :long_url %>
  <%= text_input f, :long_url %>
  <%= error_tag f, :long_url %>

  <%= label f, :hash %>
  <%= text_input f, :hash %>
  <%= error_tag f, :hash %>

  <%= submit "Save", phx_disable_with: "Saving..." %>
</form>
